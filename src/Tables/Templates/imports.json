{
    "routePrefix": "import",
    "crtNo": true,
    "appends": [
        "entries"
    ],
    "defaultSortDirection": "desc",
    "strip": [
        "created_by"
    ],
    "buttons": [
        "excel",
        {
            "name": "cancel",
            "type": "row",
            "icon": "ban",
            "method": "PATCH",
            "class": "is-row-button has-text-danger",
            "fullRoute": "import.cancel",
            "action": "ajax",
            "confirmation": true
        },
        {
            "name": "restart",
            "type": "row",
            "icon": "sync",
            "method": "PATCH",
            "class": "is-row-button",
            "fullRoute": "import.restart",
            "action": "ajax",
            "confirmation": true
        },
        {
            "name": "download-rejected",
            "type": "row",
            "icon": "cloud-download-alt",
            "event": "download-rejected",
            "class": "is-row-button has-text-danger"
        },
        "download",
        "destroy"
    ],
    "filters": [
        {
            "label": "Users",
            "data": "data_imports.created_by",
            "type": "select",
            "value": [],
            "selectLabel": "person.name",
            "route": "administration.users.options",
            "multiple": true
        }
    ],
    "columns": [
        {
            "label": "Type",
            "name": "type",
            "data": "type",
            "meta": [
                "method"
            ]
        },
        {
            "label": "File name",
            "name": "file.original_name",
            "data": "file.original_name",
            "meta": [
                "searchable"
            ]
        },
        {
            "label": "Status",
            "name": "status",
            "data": "data_imports.status",
            "enum": "LaravelEnso\\DataImport\\Enums\\Status",
            "meta": [
                "sortable",
                "slot"
            ]
        },
        {
            "label": "Entries",
            "name": "entries",
            "data": "data_imports.entries",
            "meta": [
                "slot"
            ]
        },
        {
            "label": "Successful",
            "name": "successful",
            "data": "data_imports.successful",
            "meta": [
                "slot"
            ]
        },
        {
            "label": "Failed",
            "name": "failed",
            "data": "data_imports.failed",
            "meta": [
                "slot"
            ]
        },
        {
            "label": "Date",
            "name": "created_at",
            "data": "data_imports.created_at",
            "meta": [
                "filterable",
                "sortable",
                "date"
            ]
        },
        {
            "label": "Time",
            "name": "time",
            "data": "data_imports.time"
        },
        {
            "label": "Duration",
            "name": "duration",
            "data": "duration",
            "meta": [
                "sortable"
            ]
        },
        {
            "label": "By",
            "name": "createdBy",
            "data": "data_imports.createdBy",
            "resource": "LaravelEnso\\Users\\Http\\Resources\\User",
            "meta": [
                "slot",
                "notExportable"
            ]
        },
        {
            "label": "Created By",
            "name": "createdBy.person.name",
            "data": "data_imports.createdBy",
            "meta": [
                "rogue"
            ]
        }
    ]
}